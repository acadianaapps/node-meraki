{
  "name": "node-meraki",
  "version": "1.0.0",
  "description": "Meraki wrapper for nodejs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon node index.js",
    "start": "node index.js",
    "deps": "npm-check -u",
    "ci": "jest --watch",
    "test": "jest",
    "lint": "eslint .",
    "jsdoc": "jsdoc -c jsdoc.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbaertschi/node-meraki.git"
  },
  "keywords": [
    "meraki",
    "polling",
    "rest",
    "api"
  ],
  "author": "Michael BÃ¤rtschi <michael.baertschi@zebbra.ch> (http://zebbra.ch)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mbaertschi/node-meraki/issues"
  },
  "homepage": "https://github.com/mbaertschi/node-meraki#readme",
  "engines": {
    "node": ">= 8.9.x"
  },
  "dependencies": {},
  "devDependencies": {
    "docdash": "^0.4.0",
    "eslint": "^4.18.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.3.0",
    "jest-cli": "^22.3.0",
    "jest-junit": "^3.6.0",
    "jest-plugins": "^2.7.0",
    "jest-plugins-recommended": "^2.7.0",
    "nodemon": "^1.15.0",
    "npm-check": "^5.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": ".",
    "roots": [
      "<rootDir>/test/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "reports/coverage",
    "testResultsProcessor": "jest-junit",
    "setupTestFrameworkScriptFile": "<rootDir>/jest-plugins.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/nock"
    ]
  },
  "jest-junit": {
    "output": "reports/junit/js-test-results.xml"
  }
}
