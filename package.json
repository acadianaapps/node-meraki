{
  "name": "node-meraki",
  "version": "5.2.1",
  "description": "Meraki wrapper for nodejs",
  "main": "./lib/index.js",
  "scripts": {
    "build": "node_modules/.bin/babel src -d lib",
    "ci": "NODE_ENV=test node_modules/.bin/jest --watch",
    "deps": "yarn upgrade-interactive --latest",
    "jsdoc": "node_modules/.bin/jsdoc -c jsdoc.config.json",
    "lint": "node_modules/.bin/eslint .",
    "test": "NODE_ENV=test node_modules/.bin/jest",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zebbra-repos/node-meraki.git"
  },
  "keywords": [
    "meraki",
    "polling",
    "rest",
    "api"
  ],
  "author": "Michael BÃ¤rtschi <michael.baertschi@zebbra.ch> (http://zebbra.ch)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zebbra-repos/node-meraki/issues"
  },
  "homepage": "https://github.com/zebbra-repos/node-meraki#readme",
  "engines": {
    "node": ">= 8.9.x"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bottleneck": "^2.16.2",
    "debug": "^4.1.1",
    "json-bigint": "^0.3.0",
    "qs": "^6.6.0",
    "set-cookie-parser": "^2.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.1.0",
    "config": "^3.0.1",
    "docdash": "^1.0.3",
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "jest-environment-node": "^24.0.0",
    "jest-junit": "^6.3.0",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.11",
    "node-vcr": "^2.3.1",
    "pre-commit": "^1.2.2"
  },
  "jest": {
    "testEnvironment": "<rootDir>/test/sandbox.js",
    "globalSetup": "<rootDir>/test/setup.js",
    "globalTeardown": "<rootDir>/test/teardown.js",
    "rootDir": ".",
    "roots": [
      "<rootDir>/test/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "reports/coverage",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/global.config.js"
    ]
  },
  "jest-junit": {
    "output": "reports/junit/js-test-results.xml"
  },
  "pre-commit": [
    "test",
    "lint"
  ]
}
