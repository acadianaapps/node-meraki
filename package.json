{
  "name": "node-meraki",
  "version": "4.0.0",
  "description": "Meraki wrapper for nodejs",
  "main": "./lib/index.js",
  "scripts": {
    "build": "node_modules/.bin/babel src -d lib",
    "ci": "NODE_ENV=test node_modules/.bin/jest --watch",
    "deps": "node_modules/.bin/npm-check -u",
    "deps:prod": "node_modules/.bin/npm-check -s",
    "dev": "NODE_ENV=development DEBUG=node-meraki* node_modules/.bin/nodemon ./src/index.js",
    "jsdoc": "node_modules/.bin/jsdoc -c jsdoc.config.json",
    "lint": "node_modules/.bin/eslint .",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zebbra-repos/node-meraki.git"
  },
  "keywords": [
    "meraki",
    "polling",
    "rest",
    "api"
  ],
  "author": "Michael BÃ¤rtschi <michael.baertschi@zebbra.ch> (http://zebbra.ch)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zebbra-repos/node-meraki/issues"
  },
  "homepage": "https://github.com/zebbra-repos/node-meraki#readme",
  "engines": {
    "node": ">= 8.9.x"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bottleneck": "^2.8.0",
    "debug": "^3.1.0",
    "json-bigint": "^0.3.0",
    "qs": "^6.5.2",
    "set-cookie-parser": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "config": "^2.0.1",
    "docdash": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "jest-environment-node": "^23.4.0",
    "jest-junit": "^5.1.0",
    "jest-plugins": "^2.9.0",
    "jest-plugins-recommended": "^2.9.0",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.10",
    "node-vcr": "^2.2.0",
    "nodemon": "^1.18.4",
    "npm-check": "^5.8.0",
    "pre-commit": "^1.2.2"
  },
  "jest": {
    "testEnvironment": "<rootDir>/test/sandbox.js",
    "globalSetup": "<rootDir>/test/setup.js",
    "globalTeardown": "<rootDir>/test/teardown.js",
    "rootDir": ".",
    "roots": [
      "<rootDir>/test/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "reports/coverage",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/global.config.js"
  },
  "jest-junit": {
    "output": "reports/junit/js-test-results.xml"
  },
  "pre-commit": [
    "deps:prod",
    "test",
    "lint"
  ]
}
